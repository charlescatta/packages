{
  "name": "@bpinternal/entities",
  "version": "0.0.1",
  "description": "Botpress Entities Extractor",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm run build:browser && pnpm run build:node && tsc",
    "build:node": "wasm-pack build --target nodejs --release --out-dir pkg.node",
    "build:browser": "wasm-pack build --target web --release --out-dir pkg.web",
    "dev": "ts-node -T ./src/index.ts",
    "start": "node ./dist/index.js",
    "test": "jest --roots ./dist --testPathIgnorePatterns \\.util\\.test\\.js && cargo test"
  },
  "dependencies": {
    "browser-or-node": "^2.1.1",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.1",
    "jest": "^26.6.3",
    "typescript": "^5.0.4",
    "wasm-pack": "^0.11.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
