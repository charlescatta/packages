{
  "name": "@bpinternal/entities",
  "version": "0.0.1",
  "description": "Botpress Entities Extractor",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm run build:node && pnpm run build:web && tsc",
    "build:node": "wasm-pack build --verbose --target nodejs -d pkg/node --release",
    "build:web": "wasm-pack build --verbose --target web -d pkg/web --release",
    "dev": "ts-node -T ./src/index.ts",
    "start": "node ./dist/index.js",
    "test": "jest --roots ./dist --testPathIgnorePatterns \\.util\\.test\\.js && cargo test"
  },
  "dependencies": {
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.1",
    "jest": "^26.6.3",
    "typescript": "^5.0.4",
    "wasm-pack": "^0.12.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
